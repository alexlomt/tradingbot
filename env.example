# Network Configuration
RPC_ENDPOINT=https://api.mainnet-beta.solana.com
WS_ENDPOINT=wss://api.mainnet-beta.solana.com
COMMITMENT=confirmed
CLUSTER=mainnet-beta

# Wallet Configuration
WALLET_PATH=/path/to/wallet.json
WALLET_PUBLIC_KEY=your_public_key_here

# Redis Configuration
REDIS_ENABLED=true
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_TLS=false

# Trading Parameters
MAX_CONCURRENT_TRADES=5
MAX_DAILY_TRADES=50
MAX_POSITION_SIZE=1000000000
DAILY_VOLUME_LIMIT=10000000000
DEFAULT_SLIPPAGE=1.0
MIN_LIQUIDITY_USD=100000000
EMERGENCY_CLOSE_ALL=false

# Risk Management
TAKE_PROFIT=5.0
STOP_LOSS=2.5
MAX_DRAWDOWN=10.0
MAX_LEVERAGE=2.0
COOLDOWN_PERIOD=300000

# Transaction Settings
USE_WARP_TRANSACTIONS=true
USE_JITO_TRANSACTIONS=true
USE_CUSTOM_FEE=true
CUSTOM_FEE=10000
COMPUTE_UNIT_LIMIT=200000
COMPUTE_UNIT_PRICE=1000
SKIP_PREFLIGHT=false
MAX_RETRIES=3
MAX_TIMEOUT=30000

# Market Data Settings
MARKET_UPDATE_INTERVAL=1000
PRICE_UPDATE_INTERVAL=500
LIQUIDITY_CHECK_INTERVAL=5000
VOLATILITY_WINDOW=86400000

# Snipe List Configuration
SNIPE_LIST_ENABLED=true
SNIPE_LIST_MAX_ENTRIES=1000
SNIPE_LIST_AUTO_REMOVE_DAYS=30
SNIPE_LIST_PERSIST=true
SNIPE_LIST_SYNC_INTERVAL=300000
SNIPE_LIST_BACKUP_INTERVAL=3600000

# Cache Settings
CACHE_ENABLED=true
CACHE_TTL=86400
CACHE_MAX_SIZE=10000
CACHE_UPDATE_INTERVAL=300000

# Logging and Monitoring
LOG_LEVEL=info
SENTRY_ENABLED=true
SENTRY_DSN=your_sentry_dsn_here
METRICS_ENABLED=true
METRICS_PORT=9090

# Feature Flags
AUTOMATIC_TRADING_ENABLED=true
RISK_MANAGEMENT_ENABLED=true
WARP_INTEGRATION_ENABLED=true
JITO_INTEGRATION_ENABLED=true
METRICS_DASHBOARD_ENABLED=true
